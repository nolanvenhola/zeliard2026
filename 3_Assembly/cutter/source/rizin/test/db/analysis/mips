|     `===< 0x00400584      balc[16] sym.decrypt
|      ||   0x00400586      move  a3, a0
|     ,===< 0x00400588      bnezc a3, loc..L12
|     |||   0x0040058a      lapc.h a0, loc..LC2
|     |||   0x0040058e      lw    a3, 0x2c(gp)                         ; [0x420048:4]=0x4003c0 ; reloc.puts
|     |||   0x00400592      jalrc ra, a3
