NAME=dmi ld
FILE=/bin/ls
ARGS=-d
CMDS=<<EOF
dmiQ ld | grep 'exception' | sort
dmiQ ld _dl_signal_error
EOF
EXPECT=<<EOF
_dl_catch_exception
_dl_exception_create
_dl_exception_create_format
_dl_exception_free
_dl_signal_exception
_dl_signal_error
EOF
RUN

NAME=dmia ld
FILE=/bin/ls
ARGS=-d
CMDS=<<EOF
dmiaQ ld~la_activity
EOF
EXPECT=<<EOF
la_activity
EOF
RUN

