NAME=pi pdq with ahi2
FILE=malloc://1024
CMDS=<<EOF
e asm.bytes=true
e asm.arch=x86
e asm.bits=64
wx 48b96e74646c6c2e646c
ahi s
pi 1
pdq 1
EOF
EXPECT=<<EOF
mov rcx, 'ld.lldtn'
0x00000000 48b96e74646c6c2e646c  mov rcx, 'ld.lldtn'
EOF
RUN

NAME=uafbug pi pdq
FILE=malloc://1024
CMDS=<<EOF
e asm.bytes=true
e asm.arch=x86
e asm.bits=64
wx 48b96e74646c6c2e646c
ahi i
pi 1
ahi s
pi 1
pdq 1
ia > /dev/null
pdq 1
ia > /dev/null
pdq 1
EOF
EXPECT=<<EOF
mov rcx, 110.116.100.108
mov rcx, 'ld.lldtn'
0x00000000 48b96e74646c6c2e646c  mov rcx, 'ld.lldtn'
0x00000000 48b96e74646c6c2e646c  mov rcx, 'ld.lldtn'
0x00000000 48b96e74646c6c2e646c  mov rcx, 'ld.lldtn'
EOF
RUN
