NAME=Print the lang plugins
FILE==
CMDS=<<EOF
Ll~^pipe
Ll~lib
# Llj cannot filter for json
Llt:name/eq/pipe
Llq~^pipe
Llq~lib
EOF
EXPECT=<<EOF
pipe: Use #!pipe node script.js (LGPL)
lib: Load libs directly into rizin (LGPL)
name description               license 
---------------------------------------
pipe Use #!pipe node script.js LGPL
pipe
lib
EOF
RUN
