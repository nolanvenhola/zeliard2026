NAME=consistency btw /z and /x at 0x20k multiple
FILE=--
CMDS=<<EOF
. scripts/search-consistency-test.rz
$str=$abc+d
$bin=$malloc://0x40000+10
$setup=$"w `$str` @@=0x20000 0x40000"
$cmd_z=$/z ${str}
$cmd_x=$/x ${bytes}
.(search-consistency-test)
EOF
EXPECT=<<EOF
[0x0, 0x207ff): 1
[0x20000, 0x4000a): 2
