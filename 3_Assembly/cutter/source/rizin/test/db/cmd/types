var int32_t var_14h @ stack - 0x14
var int32_t var_10h @ stack - 0x10
arg int sarg2 @ stack + 0x4
arg int sarg3 @ stack + 0x8
arg int sarg0 @ ecx
arg int sarg1 @ edx
name: fastcalled
call-convention: fastcall
EOF
RUN

