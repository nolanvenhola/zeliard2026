NAME=td uint64_t
FILE==
CMDS=<<EOF
e asm.arch=x86
wx 0xa00f
wx 0x401f @ 0x4
wx 0800000000000080 @ 0x8
td "struct foo {int a; int b; uint64_t c;};"
avga goo "struct foo" @ 0x0
pd 1
EOF
EXPECT=<<EOF
(foo goo)
 a : 0x00000000 = 4000
 b : 0x00000004 = 8000
 c : 0x00000008 = (qword)0x8000000000000008
EOF
RUN

