|           ; var int64_t var_ch  { > 0xa} @ stack - 0xc
|           ; var int64_t var_1ch  { > 0x0 && <= 0x9} @ stack - 0x1c
|           ; var int64_t var_ch  { > 0x64 && <= 0xc7} @ stack - 0xc
|           ; var int64_t var_ch  { > 0xff && <= 0x12b || > 0x14 && <= 0x31 || > 0x6f && <= 0xdd} @ stack - 0xc
EOF
